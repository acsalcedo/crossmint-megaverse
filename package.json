    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "jest": "^29.7.0",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.2",
    "ts-jest": "^29.3.1",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0",
